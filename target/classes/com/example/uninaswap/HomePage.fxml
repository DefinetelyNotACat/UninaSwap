<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<StackPane prefHeight="400.0" prefWidth="800.0"
           xmlns="http://javafx.com/javafx/24.0.1"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.example.uninaswap.boundary.HomePageBoundary"
           stylesheets="@banners.css">

    <VBox alignment="TOP_CENTER">
        <HBox alignment="CENTER">
            <fx:include source="navbar.fxml" />
        </HBox>

    </VBox>

    <VBox alignment="TOP_CENTER" pickOnBounds="false">
        <StackPane.margin>
            <Insets top="15.0" />
        </StackPane.margin>

        <HBox alignment="CENTER" pickOnBounds="false">
            <fx:include source="banners.fxml" fx:id="notifica" maxWidth="-Infinity" />
        </HBox>
    </VBox>

</StackPane>